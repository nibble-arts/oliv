<?xml version="1.0" encoding="utf-8"?>
<!-- List of pages found
Used for re-/translating by the router

This list is updated, when a page is created, edited or deleted
if the route does not find a page in this file, it scans the page directory and inserts the found page here.

The <define> part of the page definition is copied

The status defines the visibility:
		hidden		don't show
		prepared	is ready to go online, but dont' show
		public		show
-->
<pages>
	<structure>
		<main>
			<index/>
			<documentation submenu="documentation"/>
		</main>

		<documentation>
			<function submenu="function"/> <!-- visible="show" always display submenu -->
			<extensions submenu="extensions"/> <!-- submenu="menuName" ... user menu as submenu -->
			<templates submenu="templates"/>
			<content submenu="content"/>
			<administration submenu="administration"/>
		</documentation>

		<function>
			<function_basics mod="article" page="function"/>
			<core mod="article" page="function"/>
			<init mod="article" page="function"/>
			<route mod="article" page="function"/>
			<page mod="article" page="function"/>
			<preprocessor mod="article" page="function"/>
			<render mod="article" page="function"/>
			<postprocessor mod="article" page="function"/>
		</function>

		<extensions>
	 		<plugins submenu="plugins"/>
	 		<modules submenu="modules"/>
	 	</extensions>
		
		<plugins>
			<plugin_basics mod="article" page="plugins"/>
			<search_plugin mod="article" page="plugins"/>
			<page_plugin mod="article" page="plugins"/>
			<route_plugin mod="article" page="plugins"/>
		</plugins>
		
		<modules>
			<module_basics/>
			<article_module/>
			<menu_module/>
			<login_module/>
			<search_module/>
			<toolbar_module/>
		</modules>
		
		<templates>
			<template_basics/>
			<skins/>
			<page_templates/>
			<global_templates/>
		</templates>

		<content>
			<content_basics/>
			<static_content/>
			<module_content/>
		</content>
	
		<administration>
			<admin_basics/>
			<permissions/>
			<text_edit/>
			<template_edit/>
		</administration>
	</structure>

